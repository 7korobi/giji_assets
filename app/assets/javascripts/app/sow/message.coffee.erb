Cache.rule.item.set <%= JSON.generate MESSAGE.sow %>
if gon?.items?
  Cache.rule.item.merge gon.items

<% MESSAGE.module.sow.each do |name| %>
GUI.items_module "<%= name %>"
<% end %>

<%
{ "RAILS.mob" => "<ul>" + RAILS.mob.map {|k, mob| "<li><code>#{mob.CAPTION}</code><kbd>#{mob.HELP}</kbd>" }.join + "</ul>",
  "RAILS.winner.WIN_WOLF.CAPTION-HUMAN" => RAILS.winner.WIN_WOLF.CAPTION + "の人間",
  "RAILS.winner.WIN_EVIL.CAPTION-HUMAN" => RAILS.winner.WIN_EVIL.CAPTION + "の人間",
  "RAILS.winner.WIN_WOLF.HELP" => RAILS.winner.WIN_WOLF.HELP,
  "RAILS.winner.WIN_EVIL.HELP" => RAILS.winner.WIN_EVIL.HELP,
  "RAILS.winner.WIN_PIXI.HELP" => RAILS.winner.WIN_PIXI.HELP,
  "RAILS.winner.WIN_HUMAN.HELP" => RAILS.winner.WIN_HUMAN.HELP,
}.each do |key, log| %>
for item in Cache.items.where(log:"<%= key %>").list()
  item.log = <%= log.to_json %>
<% end %>

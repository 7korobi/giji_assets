Mem.rule.item.set <%- JSON.stringify(MESSAGE.rails) %>
if gon?.items?
  Mem.rule.item.merge gon.items

for name in <%- JSON.stringify(MESSAGE.module.rails) %>
  GUI.items_module name

rule_accordion = (type)->
  GUI.if_exist "#rule-#{type}", (dom)->
    m.mount dom,
      controller: ->
      view: ->
        GUI.accordion type, RULE[type].list

rule_accordion "nation"
rule_accordion "village"
rule_accordion "maker"
rule_accordion "player"

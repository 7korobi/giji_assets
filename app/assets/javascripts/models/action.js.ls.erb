      all

  @default ->

  index = 0
  @deploy (o)->
    unless o.index
      o.index = index += 1
    o._id = o.index
    o.target ?= true

  @map_reduce (o)->

Mem.rule.action.set <%= JSON.generate SET_ACTIONS %>

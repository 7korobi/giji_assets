<%# coding: UTF-8 -%>
package SWResource_TAG;

#----------------------------------------
# キャラクタータグ
#----------------------------------------

sub GetTag {
  # タグの名前
  my %tag_name = (
<%  @tags.each do |tag, o|  %>
    '<%= tag %>' => '<%= o.label %>',
<%  end %>
  );

  my %tagset = (
    TAG_NAME       => \%tag_name,
  );

  return \%tagset;
}

1;

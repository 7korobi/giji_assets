new Mem.Rule("action").schema ->
  @order "index"
  @scope (all)->
    for_form: (mestype, format)->
      all

  class @model extends @model
    (o, { rowid })->
      unless @index
        @index = rowid
      @_id = @index
      @target ?= true

Mem.Collection.action.set <%- JSON.stringify(SET_ACTIONS) %>
